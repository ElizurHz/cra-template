{
  "name": "cra-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "axios": "^0.19.0",
    "connected-react-router": "^6.5.0",
    "dot-prop-immutable-chain": "0.0.3",
    "history": "^4.9.0",
    "jest-enzyme": "^7.0.2",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.4",
    "react-dom": "^16.13.1",
    "react-redux": "^7.1.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2"
  },
  "scripts": {
    "start": "react-app-rewired start --max-old-space-size=4096",
    "build": "react-app-rewired build --max-old-space-size=4096",
    "test:watch": "react-app-rewired test --env=jsdom --watchAll",
    "test:coverage": "react-app-rewired test --env=jsdom --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/eslint ./src/*.ts ./src/*.tsx ./src/**/*.ts ./src/**/*.tsx"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "babel-plugin-import": "^1.11.2",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "customize-cra": "^0.2.12",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-flowtype": "^3.9.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "git-cz": "^3.0.1",
    "husky": "^2.3.0",
    "less": "^3.11.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^8.1.7",
    "npm-license-crawler": "^0.2.1",
    "prettier": "^1.18.2",
    "react-addons-test-utils": "^15.6.2",
    "react-app-rewired": "^2.1.3",
    "react-test-renderer": "^16.8.6",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-saga-test-plan": "^4.0.0-beta.3",
    "waait": "^1.0.5"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run test:coverage"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ]
  }
}
